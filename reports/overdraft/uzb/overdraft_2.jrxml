				<textElement verticalAlignment="Middle">
					<font fontName="DejaVu Serif" size="10" isBold="true"/>
				</textElement>
				<textFieldExpression><![CDATA["\t2.3.	Банкнинг мажбуриятлари:"]]></textFieldExpression>
			</textField>
			<textField>
