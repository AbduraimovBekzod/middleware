			<textField textAdjust="StretchHeight">
				<reportElement x="37" y="111" width="480" height="20" uuid="bb038569-f76e-4a71-a3e6-fbc1bdb7e26e"/>
				<textElement textAlignment="Center" verticalAlignment="Middle">
					<font fontName="DejaVu Serif" size="10" isBold="true"/>
				</textElement>
				<textFieldExpression><![CDATA["Кафиллик шартномаси № "+$F{number}]]></textFieldExpression>
			</textField>
			<textField>
				<reportElement positionType="Float" x="37" y="133" width="260" height="19" uuid="b8dea2ca-4134-4dbf-b90f-5a3ea3232bbf"/>
