				<textElement verticalAlignment="Top">
					<font fontName="DejaVu Serif" size="14" isBold="true" isItalic="true"/>
				</textElement>
				<text><![CDATA[Национальный Банк Внешнеэкономической деятельности Республики Узбекистан]]></text>
			</staticText>
			<textField textAdjust="StretchHeight">
				<reportElement x="40" y="119" width="480" height="20" uuid="c47d866d-2e74-43d7-a938-a35533bc86f2"/>
				<textElement textAlignment="Center" verticalAlignment="Middle">
					<font fontName="DejaVu Serif" isBold="true"/>
				</textElement>
				<textFieldExpression><![CDATA["Договор поручительства № "+$F{number}]]></textFieldExpression>
			</textField>
			<textField>
				<reportElement positionType="Float" x="40" y="147" width="240" height="19" uuid="a15bf89c-93eb-4fba-ac0f-820dcf25f973"/>
				<textElement>
